@model SAMHO.Models.Medicina

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Medicina</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PrecioMedicina" class="control-label"></label>
                <input asp-for="PrecioMedicina" class="form-control" />
                <span asp-validation-for="PrecioMedicina" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdEstadoMedicina" class="control-label"></label>
                <input asp-for="IdEstadoMedicina" class="form-control" />
                <span asp-validation-for="IdEstadoMedicina" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CantidadMedicina" class="control-label"></label>
                <input asp-for="CantidadMedicina" class="form-control" />
                <span asp-validation-for="CantidadMedicina" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FechaCaducidad" class="control-label"></label>
                <input asp-for="FechaCaducidad" class="form-control" />
                <span asp-validation-for="FechaCaducidad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NombreMedicina" class="control-label"></label>
                <input asp-for="FechaRegistro" class="form-control" />
                <span asp-validation-for="FechaRegistro" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear Medicina" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-danger">Cancelar</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
